---
import Layout from "../layouts/Layout.astro"
import Navbar from "../components/navbar.tsx"
import CarouselDemo from "../components/carousel.tsx"
import Maxim from "../components/maxim.tsx"
---

<Layout title="Home">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
		<main>

			<div class="max-w-[400px] mx-auto mt-4 md:max-w-[768px] md:mx-auto">
				<Navbar client:load />
			</div>
				<div class="max-w-[375px] mx-auto mt-4 md:max-w-[684px] md:mx-auto">
					<CarouselDemo client:load />
				</div>
				<div class="max-w-[375px] mx-auto mt-4 md:max-w-[684px] md:mx-auto">
					<Maxim client:load />
				</div>
		</main>
	</body>
</Layout>
